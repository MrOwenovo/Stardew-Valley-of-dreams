    "Target": "LooseSprites/font_bold, LooseSprites/font_colored",
    "FromFile": "assets/{{Target}}_shadowless.png",
    "When":{ "Latin Dialogue Font Change": "enabled",  "Dialogue Font Shadows": "disabled"},
