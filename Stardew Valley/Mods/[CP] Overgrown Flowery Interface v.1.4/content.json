{
    "Format": "1.19.0",
    "ConfigSchema":{
        "Title Card Options": {
            "AllowValues": "plain, floral, morefloral",
            "Default": "floral",
            "Description": "Variant options for the logo on the menu." 
          },
        "DigSpot Toggle": {
            "AllowValues": "enabled, disabled",
            "Default": "enabled",
            "Description": "Changes the digspot into a flower." 
          },
          "DigSpot Color": {
            "AllowValues": "red, pink, lilac, violet, white, black, orange, yellow, blue",
            "Default": "violet",
            "Description": "Changes the color of the digspot flower if enabled."
          },
          "Kitty Channels": {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Changes the TV channels."
          },
          "Farmer Portrait": {
            "AllowValues": "pixel, realistic, disabled",
            "Default": "pixel",
            "Description": "Changes the background of the farmer portrait found in the menu."
          },
          "Cursor Toggle": {
            "AllowValues": "enabled, disabled",
            "Default": "enabled",
            "Description": "Changes the cursors or pointers."
          },
          "Hit Box Toggle":{
            "AllowValues": "enabled, disabled",
            "Default": "enabled",
            "Description": "Changes the tool hit box."
          },
          "Gender Icons":{
            "AllowValues": "enabled, disabled",
            "Default": "enabled",
            "Description": "Recolors the gender icons to be blue and pink."
          },
          "Heart Icons":{
            "AllowValues": "enabled, disabled",
            "Default": "enabled",
            "Description": "Changes the relationship indicators on the NPC portrait."
          },
          "Billboard Toggle":{
            "AllowValues": "enabled, disabled",
            "Default": "enabled",
            "Description": "Overhauls the billboard and calendar."
          },
          "Letter Toggle":{
            "AllowValues": "enabled, disabled",
            "Default": "enabled",
            "Description": "Changes the paper for letters in the mail."
          },
          "Latin Dialogue Font Change": {
              "AllowValues": "enabled, disabled",
              "Default": "enabled",
              "Description": "Changes the Latin dialogue font (font_bold)."
          },
          "Dialogue Font Shadows":{
              "AllowValues": "enabled, disabled",
              "Default": "enabled",
              "Description": "Enabled gives the Latin dialogue font a light shadow. Disabled removes the shadow."
          },
          "Small Font Toggle":{
            "AllowValues": "enabled, disabled",
            "Default": "disabled",
            "Description": "Applies changes to the smaller sprite font the game uses. This can cause errors in the text displayed. Use only if you want the old effect."
        },

    },
    "DynamicTokens":[
          {
            "Name": "DigSpot",
            "Value": "{{Digspot Color}}",
          }
    ],
    "Changes": [
     

